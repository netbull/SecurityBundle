{
  "name" : "netbull/security-bundle",
  "description" : "NetBull Security bundle",
  "type" : "symfony-bundle",
  "authors" : [{
    "name" : "Aleksandar Dimitrov",
    "email" : "netbull2007@gmail.com"
  }],
  "license" : [
    "MIT"
  ],
  "require" : {
    "php": "^7.1.3",
    "browscap/browscap-php": "^4.1",
    "symfony/framework-bundle": "~4.0",
    "netbull/core-bundle": "^2.0"
  },
  "require-dev" : {
    "symfony/phpunit-bridge": "~4.0"
  },
  "scripts": {
    "auto-scripts": {
      "vendor/bin/browscap-php browscap:fetch": "php-script",
      "vendor/bin/browscap-php browscap:convert": "php-script"
    },
    "post-install-cmd": [
      "@auto-scripts"
    ],
    "post-update-cmd": [
      "@auto-scripts"
    ]
  },
  "autoload" : {
    "psr-4" : {
      "NetBull\\SecurityBundle\\" : ""
    }
  }
}
